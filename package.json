{
  "name": "test-app",
  "version": "1.0.0",
  "description": "A test app based on react and redux.",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --config webpack.config.prod.js",
    "start": "NODE_ENV=development webpack-dev-server",
    "server": "babel-node tools/distServer",
    "test": "mocha --colors --require tools/testSetup.js --recursive \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "cleanup": "rimraf .nyc_output coverage",
    "report": "nyc --reporter=lcov --reporter=text npm test"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Elena Sufieva",
  "license": "ISC",
